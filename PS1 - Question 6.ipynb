    "    s=s.lower()\n",
    "    if ((s[0] =='a')or (s[0] =='e')or (s[0] =='i')or (s[0] =='o')or( s[0] =='u')) :\n",
    "        print(s+'way')\n",
    "        \n",
    "    else :\n",
    "        print (s[1:]+s[0]+'ay')\n",
    "pig(\"happy\")\n",
    "pig(\"apple\")"
